{
    "functions": {
      "api/server.js": {
        "memory": 1024,
        "includeFiles": "db.json"
      }
    },
     "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db.json"]
      }
    },
  ],
      "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
},
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "api/server.js"
      }
    ]
  }
